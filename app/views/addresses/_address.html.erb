<div id="<%= dom_id address %>">
  <p>
    <strong>City:</strong>
    <%= address.city %>
  </p>

  <p>
    <strong>Voivodship:</strong>
    <%= address.voivodship %>
  </p>

  <p>
    <strong>Postcode:</strong>
    <%= address.postcode %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= address.country %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= address.street %>
  </p>

  <p>
    <strong>User:</strong>
    <%= address.user_id %>
  </p>

  <p>
    <strong>Library:</strong>
    <%= address.library_id %>
  </p>

</div>
