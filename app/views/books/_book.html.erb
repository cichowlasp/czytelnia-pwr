<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= book.type %>
  </p>

  <p>
    <strong>Publisher:</strong>
    <%= book.publisher %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= book.available %>
  </p>

  <p>
    <strong>Order:</strong>
    <%= book.order_id %>
  </p>

  <p>
    <strong>Library:</strong>
    <%= book.library_id %>
  </p>

</div>
