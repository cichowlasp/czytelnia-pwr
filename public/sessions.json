{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://127.0.0.1:3000",
  "resourcePath": "sessions",
  "apis": [
    {
      "path": "/login",
      "operations": [
        {
          "summary": "Gather a token",
          "parameters": [
            {
              "paramType": "form",
              "name": "session[index]",
              "type": "string",
              "description": "User index",
              "required": true
            },
            {
              "paramType": "form",
              "name": "session[password]",
              "type": "string",
              "description": "User password",
              "required": true
            }
          ],
          "nickname": "Sessions#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/logout",
      "operations": [
        {
          "summary": "destroy token",
          "parameters": [
            {
              "paramType": "form",
              "name": "id",
              "type": "integer",
              "description": "User id",
              "required": true
            }
          ],
          "nickname": "Sessions#destroy",
          "method": "get"
        }
      ]
    }
  ],
  "authorizations": null
}